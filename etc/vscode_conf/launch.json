    "version": "0.2.0",
    "configurations": [
        {
            "name": "(lldb) 启动",
            "type": "cppdbg",
            "request": "launch",
            "program": "${fileDirname}/${fileBasenameNoExtension}.out",
            "args": [],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": true,
            "MIMode": "lldb"
        },
        {
            "name": "Python: Current File",
            "type": "python",
            "python": "/usr/bin/python3", // 指定python路径
            "request": "launch",
            "program": "${file}",
            "env": {
                "PYTHONPATH": "",
                "PATH": "",
                "LD_LIBRARY_PATH": ""
            },
            "console": "integratedTerminal"
        },
        {
            "name": "Python: My test file",
            "type": "python",
            "python": "/usr/bin/python3", // 指定python路径
            "request": "launch",
            "program": "/path/to/my/test/file.py",
            "env": {
                "PYTHONPATH": "",
                "PATH": "",
                "LD_LIBRARY_PATH": ""
            },
            "console": "integratedTerminal"
        },
        {
            "name": "Python: Module",
            "type": "python",
            "python": "/usr/bin/python3", // 指定python路径
            "request": "launch",
            "module": "test_module",
            "env": {
                "PYTHONPATH": "XXX",
                "PATH": "XXX"
            }
        }
    ]
}
